@model Sherlock.Models.Landmark
@{
    ViewBag.Title = "Details";
}
<div class="panel panel-default">
    <div class="panel-body">

        <h1 class="biggy">@Html.DisplayFor(model => model.Title)</h1>
        <div><img src="@Html.DisplayFor(model => model.Image1)" style="width: 100%;" alt="Alternate Text" /></div>
        <div style="color: transparent;">.</div>
        <div class="panel panel-default">
            <div class="panel-heading sansText">
                <p>@Html.DisplayFor(model => model.Address1) @Html.DisplayFor(model => model.Address2)</p>
                <p>@Html.DisplayFor(model => model.City), @Html.DisplayFor(model => model.State)  @Html.DisplayFor(model => model.ZipCode)</p>
            </div>
        </div>
        <div>
            <p class="sansText">@Html.DisplayFor(model => model.Description)</p>
        </div>

        <h2>Additional Links</h2>
        <a href="@Html.DisplayFor(model => model.Links)">@Html.DisplayFor(model => model.Links)</a>

        <h2>Rating &nbsp;&nbsp;&nbsp;<span class="alert-danger">pipes</span></h2>


        <p>
            @*@Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |*@
            @Html.ActionLink("Back to List", "Index")
        </p>
    </div>
</div>